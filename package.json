{
  "name": "bears-and-bear-markets",
  "engines": {
    "node": "12.14.x",
    "npm": "6.13.4"
  },
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "axios": "^0.18.1",
    "bulma": "^0.8.1",
    "moment-timezone": "^0.5.28",
    "node-sass": "^4.13.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-scripts": "3.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "start": "node-sass --omit-source-map-url src/sass/style.sass src/css/style.css && react-scripts start",
    "build": "node-sass --omit-source-map-url src/sass/style.sass src/css/style.css && react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "css-build": "node-sass --omit-source-map-url src/sass/style.sass src/css/style.css",
    "css-watch": "npm run css-build -- --watch",
    "css-start": "npm run css-watch"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
